<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#a6aaf4"> </solid>
            <stroke android:width="1.5dp" android:color="#040438" android:dashGap="0px" android:dashWidth="0px"> </stroke>
            <corners android:radius="10dp"> </corners>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#283893"> </solid>
            <stroke android:width="1.5dp" android:color="#040438" android:dashGap="0px" android:dashWidth="0px"> </stroke>
            <corners android:radius="10dp"> </corners>
        </shape>
    </item>
</selector>